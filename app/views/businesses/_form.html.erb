<%= render "layouts/errors", :object => @business %>

<%= form_for [@type,@business] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :web_address %>
  <%= f.text_field :web_address %>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :hours %>
  <%= f.text_field :hours %>
  <%= f.submit %>
<% end %>
